        "CommandTimeout": 5000,
        "SqlCommand": "SELECT HD.FTBchCode,HD.FTXshRefExt,FNXshDocType,\r\n\tPos.FTPosRegNo,\r\n\tHD.FTXshStaApv, \r\n\tHD.FTCstCode,Cst_L.FTCstName,CstAddr_L.FTAddName,\r\n\tISNULL(HDCst.FTXshCardID,'') AS FTCstTaxNo,\t\r\n\tCst.FTCstBusiness,Cst.FTCstBchHQ,Cst.FTCstBchCode,\r\n\tCASE WHEN Cst.FTCstBusiness = '1' THEN CASE WHEN Cst.FTCstBchHQ = '1' THEN 'สำนักงานใหญ่ ' ELSE 'สาขา : ' + Cst.FTCstBchCode  END ELSE Cst.FTCstBchCode  END AS FTBchName,\t\r\n\tCst.FTCstTel,Cst.FTCstFax,\r\n\tHD.FTXshDocNo,FDXshDocDate,CONVERT(VARCHAR(10),FDXshDocDate,103) AS FTXshDocDate,CONVERT(VARCHAR(10),FDXshDocDate,8) AS FTXshDocTime,HD.FTXshRefInt,HD.FTCreateBy,\r\n\tISNULL(HD.FTSpnCode,'') AS FTSpnCode,Spn_L.FTSpnName,\r\n\tISNULL(HD.FTUsrCode,'') AS FTUsrCode,Usr_L.FTUsrName,\r\n    ISNULL(Red_L.FTUsrName,HD.FTLastUpdBy) AS FTXshRecorder,\r\n\tISNULL(HD.FTXshVATInOrEx,'') AS FTXshVATInOrEx,\r\n\tISNULL(FTXshStaDoc,'') AS FTXshStaDoc,\r\n\tISNULL(FTXshStaApv,'') AS FTXshStaApv,\r\n\tISNULL(FTXshStaPrcStk,'') AS FTXshStaPrcStk,\r\n\tISNULL(FTXshApvCode,'') AS FTXshApvCode,\r\n\tISNULL(HDCst.FTXshCtrName,'') AS FTXshCtrName,\r\n\tISNULL(FTXshRmk,'') AS FTXshRmk,\r\n\tISNULL(CstAddr_L.FTAddVersion,'') AS FTAddVersion,\r\n\tISNULL(CstAddr_L.FTAddV2Desc1,'') AS FTAddV2Desc1,\r\n\tISNULL(CstAddr_L.FTAddV2Desc2,'') AS FTAddV2Desc2,\r\n\tISNULL(CstAddr_L.FTAddRefNo,'') AS FTAddRefNo,\r\n\tISNULL(CstAddr_L.FTAddV1PvnCode,'') AS FTAddV1PvnCode, ISNULL(CstAddr_L.FTAddV1DstCode,'') AS FTAddV1DstCode,\r\n\tISNULL(CstAddr_L.FTAddV1SubDist,'') AS FTAddV1SubDist, \r\n\tISNULL(Pro_L.FTPvnName,'') AS FTPvnName,\r\n\tISNULL(Dis_L.FTDstName,'') AS FTDstName,\r\n\tISNULL(SDis_L.FTSudName,'') AS FTSudName,\r\n\tISNULL(CstAddr_L.FTAddName,'') AS FTAddName, ISNULL(CstAddr_L.FTAddV1No, '') AS FTAddV1No,\r\n\tISNULL(CstAddr_L.FTAddV1Village, '') AS FTAddV1Village,\r\n\tISNULL(CstAddr_L.FTAddV1Soi, '') AS FTAddV1Soi,\r\n\tISNULL(CstAddr_L.FTAddV1Road, '') AS FTAddV1Road,\r\n\tISNULL(CstAddr_L.FTAddV1PostCode, '') AS FTAddV1PostCode,\r\n\tISNULL(FCXshTotal,0) AS FCXshTotal,ISNULL(FCXshVat,0) AS FCXshVat,ISNULL(FCXshChg,0) - ISNULL(FCXshDis,0) AS FCXshDiscount,\r\n    ISNULL(FCXshVatable,0) + ISNULL(FCXshVat,0) AS FCXshVatAmt,\r\n\tISNULL(FCXshVatable,0) AS FCXshVatable,\r\n\tISNULL(FCXshTotalAfDisChgV,0)+ISNULL(FCXshTotalAfDisChgNV,0) AS FCXshTotalAFDis,\r\n    ISNULL(FCXshGrand,0)-ISNULL(FCXshRnd,0) AS FCXshGrandBFRnd,\r\n\tISNULL(FCXshRnd,0) AS FCXshRnd,\r\n\tISNULL(FCXshGrand,0) AS FCXshGrandAmt,\r\n\tISNULL(FCXshAmtV,0) AS FCXshAmtV,\r\n\tCASE WHEN HD.FTXshVATInOrEx='1' THEN ISNULL(FCXshTotal,0)+(ISNULL(FCXshChg,0) - ISNULL(FCXshDis,0)) ELSE (ISNULL(FCXshTotal,0)+(ISNULL(FCXshChg,0) - ISNULL(FCXshDis,0)))+HD.FCXshVat END AS FCXshGrand,\r\n    HD.FTXshGndText, \r\n\tHD.FNXshDocPrint,\r\n\tISNULL(HD.FTXshStaETax,'2') AS FTXshStaETax,\r\n\tHDCst.FNXshCrTerm,Car.FTCarRegNo,CtyL.FTCaiName AS FTCaiNameType,CbnL.FTCaiName AS FTCarBran,CmlL.FTCaiName AS FTCarModel,CclL.FTCaiName AS FTCarColor,\r\n\tJob2Ord.FTFlwLastDate,Job2Ord.FCFlwCurMileAge,Car.FTCarOwner,Oldner_L.FTCstName AS FTCstOwnName\r\n\tFROM TPSTSalHD  HD WITH (NOLOCK)  LEFT JOIN \r\n     TPSTSalHDCst HDCst WITH (NOLOCK) ON HD.FTBchCode = HDCst.FTBchCode AND HD.FTXshDocNo = HDCst.FTXshDocNo LEFT JOIN\r\n\t TSVMCar Car WITH (NOLOCK) ON HDCst.FTCarCode = Car.FTCarCode LEFT JOIN\r\n\t TSVMCarInfo_L CtyL WITH (NOLOCK) ON Car.FTCarType= CtyL.FTCaiCode\tAND CtyL.FNLngID= @pnLanguage LEFT JOIN\r\n\t TSVMCarInfo_L CbnL WITH (NOLOCK) ON Car.FTCarBrand = CbnL.FTCaiCode\tAND CbnL.FNLngID= @pnLanguage LEFT JOIN\t\r\n\t TSVMCarInfo_L CmlL WITH (NOLOCK) ON Car.FTCarModel = CmlL.FTCaiCode\tAND CmlL.FNLngID= @pnLanguage LEFT JOIN\t\r\n\t TSVMCarInfo_L CclL WITH (NOLOCK) ON Car.FTCarColor = CclL.FTCaiCode\tAND CclL.FNLngID= @pnLanguage LEFT JOIN\t\r\n\t (\r\nSELECT DecRef.FTBchCode,DecRef.FTXshDocNo,DecRef.FTXshRefDocNo ,Sfl.FTFlwLastDate,Sfl.FCFlwCurMileAge\r\n\tFROM TPSTSalHDDocRef DecRef WITH (NOLOCK) \t  \r\n\t  LEFT JOIN \r\n\t  (\r\nSELECT FTCarCode,FTFlwDocRef,CONVERT(VARCHAR(10),FDFlwLastDate,103) AS FTFlwLastDate,FCFlwCurMileAge FROM TSVTCstFollow\r\n\t  ) Sfl ON  DecRef.FTXshRefDocNo = Sfl.FTFlwDocRef\r\n\t  WHERE DecRef.FTXshRefType ='1' AND DecRef.FTXshRefKey ='Job2Ord' AND  DecRef.FTXshDocNo = @ptDocNo AND DecRef.FTBchCode = @ptDocBch\r\n     ) Job2Ord ON HD.FTBchCode = Job2Ord.FTBchCode AND HD.FTXshDocNo = Job2Ord.FTXshDocNo LEFT JOIN \r\n\t \r\n\t TCNMPos Pos WITH (NOLOCK) ON HD.FTPosCode = Pos.FTPosCode AND HD.FTBchCode = Pos.FTBchCode  LEFT JOIN \r\n\t TCNMCst Cst WITH (NOLOCK) ON HD.FTCstCode = Cst.FTCstCode LEFT JOIN\r\n\t TCNMCst_L Cst_L WITH (NOLOCK) ON HD.FTCstCode = Cst_L.FTCstCode AND Cst_L.FNLngID = @pnLanguage LEFT JOIN\r\n\t TCNMCst_L Oldner_L WITH (NOLOCK) ON Car.FTCarOwner = Oldner_L.FTCstCode AND Oldner_L.FNLngID = @pnLanguage LEFT JOIN\r\n     TCNMSpn_L Spn_L WITH (NOLOCK) ON  HD.FTSpnCode = Spn_L.FTSpnCode AND Spn_L.FNLngID = @pnLanguage LEFT JOIN\r\n\t TCNMUser_L Usr_L  WITH (NOLOCK) ON HD.FTUsrCode = Usr_L.FTUsrCode AND Usr_L.FNLngID = @pnLanguage LEFT JOIN\r\n\t TCNMUser_L Red_L   WITH (NOLOCK) ON HD.FTLastUpdBy = Red_L.FTUsrCode AND Red_L.FNLngID = @pnLanguage LEFT JOIN\r\n     TCNMCstAddress_L CstAddr_L WITH (NoLock) ON HD.FTCstCode = CstAddr_L.FTCstCode AND CstAddr_L.FTAddGrpType = '1' AND CstAddr_L.FNLngID = @pnLanguage AND FTAddRefNo = (SELECT MAX(FTAddRefNo)  FROM TCNMCstAddress_L WHERE FTAddGrpType = '1'  AND  CstAddr_L.FTCstCode = FTCstCode  GROUP BY FTCstCode)\r\n\t LEFT JOIN TCNMProvince_L Pro_L WITH (NoLock) ON CstAddr_L.FTAddV1PvnCode = Pro_L.FTPvnCode AND Pro_L.FNLngID = @pnLanguage\r\n\t LEFT JOIN TCNMDistrict_L Dis_L WITH (NoLock) ON CstAddr_L.FTAddV1DstCode = Dis_L.FTDstCode AND Dis_L.FNLngID = @pnLanguage\r\n\t LEFT JOIN TCNMSubDistrict_L SDis_L WITH (NoLock) ON CstAddr_L.FTAddV1SubDist = SDis_L.FTSudCode AND SDis_L.FNLngID = @pnLanguage\r\nWHERE HD.FTXshStaDoc = '1' AND HD.FTXshDocNo = @ptDocNo AND HD.FTBchCode = @ptDocBch",
